CREATE DATABASE TESTDB;

CREATE TABLE TESTDB.STUDENT (
		studentId int PRIMARY KEY,
		name VARCHAR(15),
		gender VARCHAR(1),
		age int
)

CREATE TABLE TESTDB.VEHICLE (
		licenseNumber VARCHAR(10),
		brand VARCHAR(15),
		model VARCHAR(15),
		type VARCHAR(2),
		engineSize int
)

CREATE TABLE TESTDB.BOOK (
		isbn VARCHAR(20) PRIMARY KEY,
		title VARCHAR(20),
		author VARCHAR(20),
		pages int,
		editorial VARCHAR(15)
)

# Trying to insert a table with a missing paranthesis
CREATE TABLE TESTDB.TEACHERS 
		FName VARCHAR,
		LName VARCHAR(20)
)

# Trying to insert table without specifing the database
CREATE TABLE TEACHERS (
		FName VARCHAR,
		LName VARCHAR(20)
)

# Trying to insert a table with unknown type 'date'
CREATE TABLE TESTDB.TEACHERS (
		FName VARCHAR,
		LName DATE
)


# Trying to insert a table with one ',' extra
CREATE TABLE TESTDB.TEACHERS (
		FName VARCHAR,
		LName VARCHAR,
)

